<Window x:Class="ArreyTelefonnummer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ArreyTelefonnummer"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btntelefonnummer"  Content="Telefonnummer" HorizontalAlignment="Left" Height="30" Margin="145,299,0,0" VerticalAlignment="Top" Width="85" Click="btntelefonnummer_Click"/>
        <TextBox x:Name="tbxUtdata" HorizontalAlignment="Left" Height="105" Margin="510,194,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="175"/>
        <Label Content="Nivå" HorizontalAlignment="Left" Height="30" Margin="710,174,0,0" VerticalAlignment="Top" Width="55"/>
        <TextBox x:Name="tbxNivå"  HorizontalAlignment="Left" Height="40" Margin="725,224,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="40"/>
        <Button x:Name="btnVisa"  Content="Visa" HorizontalAlignment="Left" Height="30" Margin="665,329,0,0" VerticalAlignment="Top" Width="70" Click="btnVisa_Click"/>

    </Grid>
</Window>
